{
    "name": "vershub/laraveltranslations",
    "description": "laravel package for translatable models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "vers"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Vershub\\LaravelTranslations\\": "src/"
        }
    },
    "extra": {
        "readme": "README.md",
        "laravel": {
            "providers": [
                "Vershub\\LaravelTranslations\\LaravelTranslationsServiceProvider"
            ]
        }
    }
}
